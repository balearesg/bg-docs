{
	"name": "code-verification-auth",
	"platforms": [
		"web",
		"ios",
		"android"
	  ],
	"txt": {
		"path": "/widget/txt",
		"multilanguage": true,
		"files": "*"
	},
	"widget": {
		"element": {
			"name": "code-verification-auth"
		},
		"route": "/code/${token}",
		"is": "page",
		"sass": {
			"path": "/widget/scss",
			"files": ["*"]
		},
		"ts": {
			"path": "/widget/ts",
			"files": ["*"]
		}
	}
}
